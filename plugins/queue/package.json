{
  "name": "@blaizejs/plugin-queue",
  "version": "2.0.0",
  "description": "Background job processing plugin for BlaizeJS with priority scheduling and SSE monitoring",
  "author": "BlaizeJS Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/jleajones/blaize.git",
    "directory": "plugins/queue"
  },
  "homepage": "https://github.com/jleajones/blaize/tree/main/plugins/queue#readme",
  "bugs": {
    "url": "https://github.com/jleajones/blaize/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "test": "vitest run --typecheck",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@datastructures-js/priority-queue": "^6.3.1",
    "zod": "^3.23.0"
  },
  "peerDependencies": {
    "blaizejs": "^0.9.1"
  },
  "devDependencies": {
    "blaizejs": "workspace:*",
    "@blaizejs/adapter-redis": "workspace:*",
    "@blaizejs/eslint-config": "workspace:*",
    "@blaizejs/testing-utils": "workspace:*",
    "@blaizejs/tsconfig": "workspace:*",
    "@blaizejs/tsup-config": "workspace:*",
    "@blaizejs/vitest-config": "workspace:*",
    "@vitest/coverage-v8": "^3.1.3",
    "vitest": "^3.1.3",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "blaizejs",
    "plugin",
    "queue",
    "jobs",
    "background-jobs",
    "task-queue",
    "priority-queue",
    "job-processing",
    "sse",
    "server-sent-events",
    "retry",
    "typescript"
  ],
  "license": "MIT"
}
