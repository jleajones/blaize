{
  "name": "@blaizejs/plugin-metrics",
  "version": "0.1.2",
  "description": "Production-ready metrics collection and observability plugin for BlaizeJS",
  "author": "J.Lea-Jones <jason@careymarcel.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jleajones/blaize.git",
    "directory": "plugins/metrics"
  },
  "homepage": "https://github.com/jleajones/blaize/tree/main/plugins/metrics#readme",
  "bugs": {
    "url": "https://github.com/jleajones/blaize/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./routes": {
      "types": "./dist/routes.d.ts",
      "import": "./dist/routes.js",
      "require": "./dist/routes.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "test": "vitest run --typecheck",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "blaizejs": "^0.5.3"
  },
  "devDependencies": {
    "blaizejs": "workspace:*",
    "@blaizejs/eslint-config": "workspace:*",
    "@blaizejs/tsconfig": "workspace:*",
    "@blaizejs/tsup-config": "workspace:*",
    "@blaizejs/vitest-config": "workspace:*",
    "@vitest/coverage-v8": "^3.1.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "blaizejs",
    "plugin",
    "metrics",
    "observability",
    "monitoring",
    "prometheus",
    "openmetrics",
    "performance",
    "telemetry"
  ],
  "license": "MIT"
}
