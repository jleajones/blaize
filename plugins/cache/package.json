{
  "name": "@blaizejs/plugin-cache",
  "version": "2.1.0",
  "description": "Event-driven cache plugin for BlaizeJS with Redis support and multi-server coordination",
  "author": "BlaizeJS Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/jleajones/blaize.git",
    "directory": "plugins/cache"
  },
  "homepage": "https://github.com/jleajones/blaize/tree/main/plugins/cache#readme",
  "bugs": {
    "url": "https://github.com/jleajones/blaize/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "test": "vitest run --typecheck",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "ioredis": "^5.8.2",
    "zod": "^3.23.0"
  },
  "peerDependencies": {
    "blaizejs": "^0.9.2"
  },
  "devDependencies": {
    "blaizejs": "workspace:*",
    "@blaizejs/eslint-config": "workspace:*",
    "@blaizejs/testing-utils": "workspace:*",
    "@blaizejs/tsconfig": "workspace:*",
    "@blaizejs/tsup-config": "workspace:*",
    "@blaizejs/vitest-config": "workspace:*",
    "@vitest/coverage-v8": "^3.1.3",
    "vitest": "^3.1.3",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "blaizejs",
    "plugin",
    "cache",
    "redis",
    "sse",
    "server-sent-events",
    "event-driven",
    "typescript",
    "multi-server"
  ],
  "license": "MIT"
}
